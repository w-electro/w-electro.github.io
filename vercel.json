{
  "version": 2,
  "builds": [
    {
      "src": "shaysukar/server/server.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 60
      }
    }
  ],
  "routes": [
    {
      "src": "/shaysukar/menu",
      "dest": "/shaysukar/menu.html"
    },
    {
      "src": "/shaysukar/barista",
      "dest": "/shaysukar/barista.html"
    },
    {
      "src": "/shaysukar/api/(.*)",
      "dest": "/shaysukar/server/server.js"
    },
    {
      "src": "/shaysukar/(.*)",
      "dest": "/shaysukar/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
